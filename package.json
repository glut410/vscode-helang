{
  "name": "vscode-helang",
  "displayName": "Helang",
  "description": "A Next-Generation Cyber Programming Language from Li Tang.",
  "publisher": "glut410",
  "version": "0.0.2",
  "icon": "images/logo.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.40.0"
  },
  "bugs": {
		"url": "https://github.com/glut410/vscode-helang/issues"
	},
  "repository": {
		"type": "git",
		"url": "https://github.com/glut410/vscode-helang"
	},
  "keywords": [
		"Helang",
		"helang",
		"helang language",
		"he",
		"extension",
		"autocompletion"
	],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "vscode-helang",
      "aliases": ["Helang", "helang"],
      "extensions": [".he"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "vscode-helang",
      "scopeName": "source.he",
      "path": "./syntaxes/helang.tmLanguage.json"
    }]
  }
}
